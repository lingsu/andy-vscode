import { ProDescriptions } from "@ant-design/pro-components";

export default ({data}: {data:<%=(config.namespace ? config.namespace + ".": "") %><%=openApi.schema.title %>}) => {

  return (
    <ProDescriptions column={3}>
      <% openApi.props.forEach((c, i) => {
        %>
        <ProDescriptions.Item label="<%=c.description %>"  
          <% if (c.type == 'boolean') { %>
            valueEnum={
              new Map([
                [true, "是"],
                [false, "否"],
              ])
            }
          <% } %>
        >
          {data.<%=c.name %>}
        </ProDescriptions.Item>
        <%});%>
    </ProDescriptions>
  );
};
