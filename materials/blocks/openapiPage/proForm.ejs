import { ProForm,
  ProFormProps,
  ProFormDateRangePicker,
  ProFormRadio,
  ProFormSelect,
  ProFormText,
  ProFormDigit,
 } from "@ant-design/pro-components";


export default (props: ProFormProps<<%=(config.namespace ? config.namespace + ".": "") %><%=openApi.schema.title %>>) => {
  return (
    <ProForm {...props}>
      <% openApi.props.forEach((c, i) => {
        %>


        <% if (c.type == 'number') { %>
          <ProFormDigit 
          label="<%=c.description %>"
          name="<%=c.name %>"
            <% if (c.required) { %>
              rules={[{ required: true, message: '这是必填项' }]}
            <% } %>
           />
        <% } else { %>
          <ProFormText 
          label="<%=c.description %>"
          name="<%=c.name %>"
            <% if (c.required) { %>
              rules={[{ required: true, message: '这是必填项' }]}
            <% } %>
           />
        <% } %>

        
      



        <%});%>
    </ProForm>
  );
};
